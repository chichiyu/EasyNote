{
    "name": "EasyNote",
    "description": "An easy way to take notes.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup":"popup.html",
        "default_icons": {
            "16": "image/icon_16.ico",
            "32": "image/icon_32.ico",
            "48": "image/icon_48.ico",
            "128": "image/icon_128.ico"
        }
    },
    "icons": {
        "16": "image/icon_16.ico",
        "32": "image/icon_32.ico",
        "48": "image/icon_48.ico",
        "128": "image/icon_128.ico"
    },
    "permissions": ["activeTab", "storage"]

}